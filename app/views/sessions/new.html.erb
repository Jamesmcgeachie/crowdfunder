<h1 id="login">Login</h1>
<%= form_tag sessions_path do %>
	<div class="labels">
		<p><%= label_tag :email %></p>
		<p><%= label_tag :password %></p>
	</div>
	<div class="inputs">
		<p><%= text_field_tag :email, params[:email] %></p>
		<p><%= password_field_tag :password %></p>
	</div>
	<div class="submit">
	<%= submit_tag "Login" %>
	</div>
<% end %>